{
    "name": "Huzbi Image Grabber",
    "description": "Extract all images from a website",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16px.png",
        "32": "icons/32px.png",
        "64": "icons/64px.png",
        "128": "icons/128px.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "background": {}
}